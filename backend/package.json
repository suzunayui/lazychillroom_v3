{
  "name": "lazychillroom-backend",
  "version": "3.0.0",
  "description": "LazyChillRoom v3 Backend API Server",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:rollback:all": "knex migrate:rollback:all",
    "db:seed": "knex seed:run",
    "db:seed:basic": "knex seed:run --specific=00_basic_data.js,01_relationships.js,02_messages_content.js,03_settings_presence.js",
    "db:seed:dev": "NODE_ENV=development knex seed:run --specific=99_development_bulk_data.js",
    "db:reset": "npm run db:rollback:all && npm run db:migrate && npm run db:seed:basic",
    "db:reset:dev": "npm run db:rollback:all && npm run db:migrate && npm run db:seed:basic && npm run db:seed:dev",
    "db:make:migration": "knex migrate:make",
    "db:make:seed": "knex seed:make"
  },
  "keywords": [
    "chat",
    "websocket",
    "realtime",
    "express",
    "postgresql"
  ],
  "author": "suzunayui",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  }
}
